{
  "name": "mygymtracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "code:lint": "eslint .",
    "code:format": "prettier --write .",
    "code:test": "vitest run",
    "web": "vite",
    "web:build": "tsc -b && vite build",
    "ios": "npm run web:build -- --mode development && cap sync ios && cap run ios",
    "ios:build": "npm run web:build && cap sync ios",
    "android": "npm run web:build -- --mode development && cap sync android && cap run android",
    "android:build": "npm run web:build && cap sync android",
    "mobile:build": "npm run web:build && npm run android:build && npm run ios:build"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@danny270793/layout": "^0.0.2",
    "@reduxjs/toolkit": "^2.11.1",
    "@tailwindcss/vite": "^4.1.17",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.4.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.10.1",
    "redux-saga": "^1.4.2",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  }
}
